# UnimogCommunityHub Environment Variables
# Copy this file to .env and replace with your actual values

# Supabase Configuration
VITE_SUPABASE_URL=https://YOUR_PROJECT_ID.supabase.co
VITE_SUPABASE_ANON_KEY=YOUR_SUPABASE_ANON_KEY_HERE
VITE_SUPABASE_PROJECT_ID=YOUR_PROJECT_ID

# Mapbox Configuration
VITE_MAPBOX_ACCESS_TOKEN=YOUR_MAPBOX_TOKEN_HERE

# Stripe Configuration (Optional)
VITE_STRIPE_PREMIUM_MONTHLY_PRICE_ID=price_xxx
VITE_STRIPE_LIFETIME_PRICE_ID=price_xxx

# Application Configuration
VITE_APP_URL=http://localhost:5173
VITE_APP_NAME=UnimogCommunityHub

# OpenAI Configuration (Required for Barry AI Mechanic)
VITE_OPENAI_API_KEY=YOUR_OPENAI_API_KEY_HERE

# Legacy Botpress Configuration (Deprecated - replaced by OpenAI)
# VITE_BOTPRESS_BOT_ID=your_bot_id
# VITE_BOTPRESS_CLIENT_ID=your_client_id
# VITE_BOTPRESS_WEBHOOK_ID=your_webhook_id